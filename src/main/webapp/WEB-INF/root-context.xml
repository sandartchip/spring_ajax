<?xml version="1.0" encoding="UTF-8"?>


<!--

controller도 view에 관련된 bean이기 때문에 

servlet-context.xml에서 관리한다. 
-->

<web-app>
	
	<!-- 파일을 업로드 할 때 그 파일이 저장될 실제 위치. -->
	<beans:bean id="uploadPath" class="java.lang.String">
		<beans:constructor-agr value="C:\\spring_file"></beans:constructor-agr>
	</beans:bean>
	 
</web-app>
